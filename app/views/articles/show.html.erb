<!-- Intro and header -->
  <div id="blog" class="inner-content">

      <section id="page-title" class="inner-section">
        <div class="container-fluid nopadding wow fadeInRight" data-wow-delay="0.4s" data-wow-offset="10">
            <h2 class="font-accident-two-normal uppercase">GITHUB</h2>
            <h5 class="font-accident-one-bold hovercolor uppercase">open source contributions and creations</h5>

      <!-- <div class="dividewhite1"></div> -->

      <div class="row">
        <div class="col-md-2 col-lg-2">&nbsp;</div>

        <!-- Button to Github -->
          <div class="row wow fadeInDown" data-wow-delay=".4s" data-wow-offset="10">
            <div class="col-md-12">
              <div class="text-center">
                <a href="https://github.com/hkdeven" target="_blank">
                 <button class="btn btn-lg btn-lgr-str">
                   <%= image_tag "icon-github-button.png", :size => '16x16' %>
                   GITHUB PAGE
                 </button>
               </a>
              </div>
            </div>
          </div>

        <div class="dividewhite1"></div>

        <!-- Github widget -->
        <div class="col-md-2 col-lg-2">&nbsp;</div>

          <div class="col-md-8 col-lg-8 e-centered wow fadeInDown" data-wow-delay="0.6s" data-wow-offset="10">
            <div id="github-widget" data-user="hkdeven" data-display="pop_repos,contribution,calendar">
            </div>
          </div>

            <script src="https://gh-widget.oyanglul.us/index.js"></script>

        </div>

      <div class="dividewhite2"></div>

    </div>
  </section>
</div>
